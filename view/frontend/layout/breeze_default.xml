<?xml version="1.0"?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceBlock name="breeze.js">
            <arguments>
                <argument name="bundles" xsi:type="array">
                    <item name="default" xsi:type="array">
                        <item name="items" xsi:type="array">
                            <item name="Amasty_Stockstatus/js/amstockstatus" xsi:type="array">
                                <item name="path" xsi:type="string">Amasty_Stockstatus/js/amstockstatus</item>
                            </item>

                            <item name="amstockstatus-register" xsi:type="array">
                                <item name="path" xsi:type="string">Swissup_BreezeAmastyStockstatus/js/amstockstatus-register</item>
                            </item>

                            <item name="amstockstatus-swatch-renderer-mixin" xsi:type="array">
                                <item name="path" xsi:type="string">Swissup_BreezeAmastyStockstatus/js/swatch-renderer-mixin</item>
                            </item>
                        </item>
                    </item>
                </argument>
            </arguments>
        </referenceBlock>
    </body>
</page>
